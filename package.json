{
  "type": "module",
  "name": "fastify-react",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:client && npm run build:server && tsc",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "clean": "del dist && del tsconfig.tsbuildinfo",
    "dev": "nodemon src/index.ts",
    "lint": "eslint ./src",
    "format": "prettier ./src --write",
    "start": "node ./dist/index.js",
    "test": "vitest --run",
    "test:types": "tsc --noEmit",
    "test:ci": "npm run test:types; npm run coverage",
    "test:e2e": "PW_EXPERIMENTAL_TS_ESM=1 playwright test",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "fastify",
    "react",
    "typescript",
    "vite",
    "vitest",
    "trpc"
  ],
  "author": "Giacomo Rebonato",
  "license": "MIT",
  "engines": {
    "node": "^19.0.0"
  },
  "dependencies": {
    "@fastify/compress": "^6.1.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/middie": "^8.0.0",
    "@fastify/session": "^10.0.2",
    "@fastify/static": "^6.5.0",
    "@fastify/websocket": "^7.1.0",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.13.4",
    "@trpc/client": "^10.0.0-proxy-beta.26",
    "@trpc/next": "^10.0.0-proxy-beta.26",
    "@trpc/react-query": "^10.0.0-proxy-beta.26",
    "@trpc/server": "^10.0.0-proxy-beta.26",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@vitejs/plugin-react": "^2.2.0",
    "daisyui": "^2.36.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.9.2",
    "framer-motion": "^7.6.2",
    "immer": "^9.0.16",
    "jotai": "^1.9.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "react": "^18.2.0",
    "react-daisyui": "^2.4.4",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.2",
    "ts-node": "^10.9.1",
    "type-fest": "^3.1.0",
    "vite": "^3.2.1",
    "vite-plugin-html": "^3.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@swc/core": "^1.3.11",
    "@testing-library/react": "^13.4.0",
    "@types/connect": "^3.4.35",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-router": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@vitest/coverage-c8": "^0.24.3",
    "autoprefixer": "^10.4.13",
    "del-cli": "^5.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
